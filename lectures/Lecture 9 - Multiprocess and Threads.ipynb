   "source": [
    "import multiprocessing as mp\n",
    "import time\n",
    "\n",
    "def run(x):\n",
    "  return x**2\n",
    "\n",
    "start = time.time()\n",
    "\n",
    "vect = list(range(2))\n",
    "print(vect)\n",
    "pool = mp.Pool(processes=1)\n",
    "results = pool.map(run, vect)\n",
    "print(results)\n",
    "end = time.time()\n",
    "print(\"Seconds:{}\".format(end - start))\n"
   ]
  }
